{
  "name": "editor",
  "version": "1.0.0",
  "dependencies": {
    "chokidar": "^1.0.5",
    "denodeify": "^1.2.1",
    "ecstatic": "^0.8.0",
    "gitignore-parser": "0.0.2",
    "jspm": "^0.16.2",
    "livedb": "^0.5.13",
    "opn": "^3.0.2",
    "promise": "^7.0.4",
    "share": "^0.7.40",
    "websocket-stream": "matthewbauer/websocket-stream#json"
  },
  "scripts": {
    "start": "node server.js",
    "install": "jspm install -y",
    "build": "jspm bundle editor.js --inject --minify --skip-source-maps"
  },
  "bin": {
    "editor": "server.js"
  },
  "main": "server.js",
  "jspm": {
    "directories": {},
    "dependencies": {
      "chokidar": "npm:chokidar@^1.0.5",
      "clean-css": "npm:clean-css@^3.4.1",
      "codemirror": "npm:codemirror@^5.6.0",
      "css": "github:systemjs/plugin-css@^0.1.15",
      "denodeify": "npm:denodeify@^1.2.1",
      "ecstatic": "npm:ecstatic@^0.8.0",
      "fs": "github:jspm/nodelibs-fs@^0.1.2",
      "gitignore-parser": "npm:gitignore-parser@^0.0.2",
      "http": "github:jspm/nodelibs-http@^1.7.1",
      "livedb": "npm:livedb@^0.5.13",
      "mime": "npm:mime@^1.3.4",
      "opn": "npm:opn@^3.0.2",
      "setimmediate": "npm:setimmediate@^1.0.2",
      "share": "npm:share@^0.7.40",
      "websocket-stream": "npm:websocket-stream@^2.0.2"
    },
    "devDependencies": {
      "traceur": "github:jmcriffey/bower-traceur@0.0.91",
      "traceur-runtime": "github:jmcriffey/bower-traceur-runtime@0.0.91"
    },
    "overrides": {
      "npm:chokidar@1.0.5": {
        "map": {
          "fsevents": "@empty"
        }
      },
      "npm:livedb@0.5.13": {
        "map": {
          "redis": "@empty"
        },
        "shim": {
          "lib/ot": {
            "format": "cjs",
            "deps": [
              "ot-json0",
              "ot-text",
              "ot-text-tp2"
            ]
          }
        }
      },
      "npm:share@0.7.40": {
        "map": {
          "express": "@empty"
        }
      }
    }
  }
}
